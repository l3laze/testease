{
  "name": "testease",
  "version": "0.0.1-rc0",
  "description": "A simple testing tool.",
  "main": "src/index.js",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "c8": "^7.7.2",
    "chai": "^4.3.4",
    "debug": "^4.3.1",
    "mocha": "^8.3.2"
  },
  "scripts": {
    "lint": "standard *.js",
    "lint-all": "standard",
    "cleancov": "rm -rf ./coverage",
    "test-mocha": "./node_modules/mocha/bin/mocha ./tests/test.js",
    "test": "node ./tests/testease-self-exam.js",
    "coverage": "./node_modules/c8/bin/c8.js --reporter=html --reporter=text --exclude=**/test*.js $npm_package_scripts_test",
    "debug": "DEBUG=testease $npm_package_scripts_test"
  }
}
